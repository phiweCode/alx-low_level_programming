#include "main.h"
#include <stdio.h>
#include <string.h>

/**
* cap_string - capitalize 
* @s: string
* return: returns a char 
*/

int _indexOf(char a)
{

int i;

char capArr[13] = {'\n', '\t', ' ', '.', ',', ';', ',', '!', '?', '(',')', '{', '}'};

for (i = 0; i < 13; i++)
{

if (capArr[i] == a)
{
return (1);
}
}

return (0);

}

